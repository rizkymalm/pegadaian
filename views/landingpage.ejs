<!DOCTYPE html>
<html>
  <head>
    <!-- <title>Dashboard</title> -->
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1.0, maximum-scale=1.0"
    />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <%- include ('./partials/header') %>
  </head>
  <body>
    <div class="wrapper">
      <div class="lSide"><%- include ('./partials/nosidebar') %></div>
      <div class="rSide">
        <div class="head gradientr desk">
          <%- include ('./partials/topdesk') %>
        </div>
        <div class="mainbox size-std column">
          <div class="cols-12 containt" style="margin: 20px 0">
            <div style="width: 100%">
              <div class="chartbox" style="border: none;">
                  <div style="width: 70%; display: flex; height: 400px; position: relative; margin: auto;">
                    <div style="display: flex; flex: 1; height: 90%; border: 1px solid #cccccc; margin: 10px; text-align: center; cursor: pointer;" onclick="window.location.href='./achievement'">
                        <table style="width: 80%; position: relative; margin: auto;" class="increase">
                            <tr>
                                <td><img src="<%= baseurl() %>images/icon/dashboard.png" alt="" width="50%"></td>
                            </tr>
                            <tr>
                                <td><h1 style="width: 100%; align-items: center;">Achievement</h1></td>
                            </tr>
                        </table>
                    </div>
                    <div style="display: flex; flex: 1; height: 90%; border: 1px solid #cccccc; margin: 10px; text-align: center; cursor: pointer;" onclick="window.location.href='./'">
                        <table style="width: 80%; position: relative; margin: auto;" class="increase">
                            <tr>
                                <td><img src="<%= baseurl() %>images/icon/report.png" alt="" width="50%"></td>
                            </tr>
                            <tr>
                                <td><h1 style="width: 100%; align-items: center;">Findings</h1></td>
                            </tr>
                        </table>
                    </div>
                  </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </body>

  <%- include ('./partials/footer') %>
  <script src="https://canvasjs.com/assets/script/jquery.canvasjs.min.js"></script>
  <!-- <script src="<% baseurl() %>js/chart.js" type="text/javascript"></script> -->
</html>
