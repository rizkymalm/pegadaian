<!DOCTYPE html>
<html>
<head>
	<!-- <title>Dashboard</title> -->
	<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <%- include ('./partials/header') %>
</head>
<body>
<div class="wrapper">
	<div class="lSide">
		<%- include ('./partials/sidebar') %>
	</div>
	<div class="rSide">
		<div class="head gradientr desk">
			<%- include ('./partials/topdesk') %>
		</div>
		<div class="mainbox size-std column">
			<div class="title-content size-std">
				DASHBOARD - <%= login.nameses+" - "+subBranch[0].sub_branch_name %>
			</div>
			<div class="top-info column">
				<div class="containt top-info-box shadow half delay" style="background: #68baf9;">
					<div class="size-std icon-info"><img src="<%= baseurl() %>images/icon/service.png"></div>
					<div class="field-info">
						<p class="nopad size-head4"><b>Skenario Gadai</b></p>
						<p class="size-head1 nopad">1</p>
						<p class="size-std nopad">Data</p>
					</div>
				</div>
				<div class="containt top-info-box shadow half delay" style="background: #ff6d74;">
					<div class="size-std icon-info"><img src="<%= baseurl() %>images/icon/delivery.png"></div>
					<div class="field-info">
						<p class="nopad size-head4"><b>Skenario Telepon CS</b></p>
						<p class="size-head1 nopad">99</p>
						<p class="size-std nopad">Data</p>
					</div>
				</div>

				<div class="containt top-info-box shadow half delay" style="background: #63D590;">
					<div class="size-std icon-info"><img src="<%= baseurl() %>images/icon/visitors-white.png"></div>
					<div class="field-info">
						<p class="nopad size-head4"><b>Skenario Pelunasan Gadai</b></p>
						<p class="size-head1 nopad">999</p>
						<p class="size-std nopad">&nbsp;</p>
					</div>
				</div>

				<div class="containt top-info-box shadow half delay" style="background: #FFCC01;">
					<div class="size-std icon-info"><img src="<%= baseurl() %>images/icon/visitors-white.png"></div>
					<div class="field-info">
						<p class="nopad size-head4"><b>Total</b></p>
						<p class="size-head1 nopad">1099</p>
						<p class="size-std nopad">&nbsp;</p>
					</div>
				</div>
			</div>
			<div class="cols-6 containt" style="margin: 20px 0;">
				<div class="headtitle trans" style="margin-bottom: 0;">
					<p class="size-head4 nopad">Data Chart</p>
					<p class="size-std nopad">Little title</p>
				</div>
				<div style="width: 100%;">
					<div class="chartbox">
						<div id="chartContainer" style="height: 370px; max-width: 920px; margin: 0px auto;"></div>
					</div>
				</div>
			</div>
			<div class="cols-6" style="padding: 0 5px; margin: 10px 0;">
				<div class="column" style="width: 100%; padding: 10px 0;">
					<div class="spccols-2 delay square btn-icon-big" style="background-color: #ffffff; border: 1px solid #d6d5d5; text-align: center;">
						<table style="height: 100%; width: 100%; color: #959a9c;">
							<tr>
								<td>
									<a href="<%= baseurl() %>delivery/Upload">
										<img src="<%= baseurl() %>images/icon/upload-delivery.png" alt="Upload Project" style="width: 55%; vertical-align: middle;">
										<br><br>
										<b>Download Rawdata</b>
									</a>
								</td>
							</tr>
						</table>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
</body>
<script>
    window.onload = function () {
    
    var options = {
        animationEnabled: true,
        theme: "light2",
        title:{
            text: "Chart"
        },
        axisX:{
            valueFormatString: "DD MMM"
        },
        axisY: {
            title: "Number of Sales",
            suffix: "K",
            minimum: 30
        },
        toolTip:{
            shared:true
        },  
        legend:{
            cursor:"pointer",
            verticalAlign: "bottom",
            horizontalAlign: "left",
            dockInsidePlotArea: true,
            itemclick: toogleDataSeries
        },
        data: [{
            type: "line",
            showInLegend: true,
            name: "Skenario Gadai",
            markerType: "square",
            xValueFormatString: "DD MMM, YYYY",
            color: "#F08080",
            lineDashType: "dash",
            yValueFormatString: "#,##0K",
            dataPoints: [
                { x: new Date(2020, 06, 1), y: 63 },
                { x: new Date(2020, 06, 2), y: 50 },
                { x: new Date(2020, 06, 3), y: 40 },
                { x: new Date(2020, 06, 4), y: 70 },
                { x: new Date(2020, 06, 5), y: 67 },
                { x: new Date(2020, 06, 6), y: 70 },
                { x: new Date(2020, 06, 7), y: 50 },
                { x: new Date(2020, 06, 8), y: 43 },
                { x: new Date(2020, 06, 9), y: 75 },
                { x: new Date(2020, 06, 10), y: 90 },
                { x: new Date(2020, 06, 11), y: 70 },
                { x: new Date(2020, 06, 12), y: 70 },
                { x: new Date(2020, 06, 13), y: 42 },
                { x: new Date(2020, 06, 14), y: 70 },
                { x: new Date(2020, 06, 15), y: 70 }
            ]
        },
        {
            type: "line",
            showInLegend: true,
            name: "Skenarion Telepon CS",
            lineDashType: "dash",
            yValueFormatString: "#,##0K",
            dataPoints: [
                { x: new Date(2020, 06, 1), y: 60 },
                { x: new Date(2020, 06, 2), y: 57 },
                { x: new Date(2020, 06, 3), y: 51 },
                { x: new Date(2020, 06, 4), y: 56 },
                { x: new Date(2020, 06, 5), y: 54 },
                { x: new Date(2020, 06, 6), y: 55 },
                { x: new Date(2020, 06, 7), y: 54 },
                { x: new Date(2020, 06, 8), y: 69 },
                { x: new Date(2020, 06, 9), y: 65 },
                { x: new Date(2020, 06, 10), y: 66 },
                { x: new Date(2020, 06, 11), y: 63 },
                { x: new Date(2020, 06, 12), y: 67 },
                { x: new Date(2020, 06, 13), y: 66 },
                { x: new Date(2020, 06, 14), y: 56 },
                { x: new Date(2020, 06, 15), y: 64 }
            ]
        }]
    };
    $("#chartContainer").CanvasJSChart(options);
    
    function toogleDataSeries(e){
        if (typeof(e.dataSeries.visible) === "undefined" || e.dataSeries.visible) {
            e.dataSeries.visible = false;
        } else{
            e.dataSeries.visible = true;
        }
        e.chart.render();
    }
    
    }
    </script>
<%- include ('./partials/footer') %>
<script src="https://canvasjs.com/assets/script/jquery.canvasjs.min.js"></script>
<script src="<% baseurl() %>js/chart.js" type="text/javascript"></script>
</html>