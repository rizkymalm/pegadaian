<script>
    window.onload = function () {
    
        var options = {
	title: {
		text: "Actual Achievement"              
    },
    exportEnabled: true,
    animationEnabled: true,
	data: [              
	{
		// Change type to "doughnut", "line", "splineArea", etc.
        type: "column",
		dataPoints: [
			{ label: "Skenario Gadai",  y: <%= ach[0].gadai  %>  },
			{ label: "Skenario Telepon", y: <%= ach[0].telepon  %>  },
			{ label: "Skenario Pelunasan", y: <%= ach[0].pelunasan  %>  }
		]
	}
	]
};
    $("#chartContainer").CanvasJSChart(options);

    var achievement_2 = {
        title: {
            text: "Percentage Achievement"
        },
        exportEnabled: true,
        animationEnabled: true,
        data: [{
            type: "column",
            legendText: "{label} %",
            indexLabelFontSize: 16,
            indexLabel: "{label} - {y}%",
            dataPoints: [
                { y: <%= percentageach[0].gadai %>, label: "Skenario Gadai" },
                { y: <%= percentageach[0].telepon %>, label: "Skenario Telepon" },
                { y: <%= percentageach[0].pelunasan %>, label: "Skenario Pelunasan" }
            ]
        }]
    };
    $("#achievement_2").CanvasJSChart(achievement_2);

    
    function toogleDataSeries(e){
        if (typeof(e.dataSeries.visible) === "undefined" || e.dataSeries.visible) {
            e.dataSeries.visible = false;
        } else{
            e.dataSeries.visible = true;
        }
        e.chart.render();
    }
    
    }
    </script>
