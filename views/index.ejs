<!DOCTYPE html>
<html>
<head>
	<!-- <title>Dashboard</title> -->
	<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <%- include ('./partials/header') %>
</head>
<body>
<div class="wrapper">
	<div class="lSide">
		<%- include ('./partials/sidebar') %>
	</div>
	<div class="rSide">
		<div class="head gradientr desk">
			<%- include ('./partials/topdesk') %>
		</div>
		<div class="mainbox size-std column">
			<div class="title-content size-std">
				DASHBOARD - <%= login.nameses %>
			</div>
			<div class="cols-12 containt" style="margin: 20px 0;">
				<div style="width: 100%;">
					<div class="chartbox">
						<div id="chartContainer" style="height: 370px; max-width: 920px; margin: 0px auto;"></div>
					</div>
				</div>
            </div>
            <div class="cols-12 containt" style="margin: 20px 0;">
				<div style="width: 100%;">
					<div class="chartbox">
						<div id="achievement_2" style="height: 370px; max-width: 100%; margin: 0px auto;"></div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
</body>
<script>
    window.onload = function () {
	
	CanvasJS.addColorSet("flexmonsterStyle",
            [//colorSet Array
            "#EE232C",
            "#478EC8",
            "#FF8F00",
            "#4DBF5A",
            "#E042A6",
            "#59BBAB",
            "#B96BDD",
            "#EDC200"
    ]);

    var options = {
	title: {
		text: "Overall Performance (2017-2020)"              
	},
	colorSet: "flexmonsterStyle",
    exportEnabled: true,
	animationEnabled: true,
	axisY:{
		minimum: 0,
		maximum: 100,
	},
	axisX: {
        title: " ",
        labelFontSize: 16,
        labelWrap: true
    },
	data: [              
	{
		type: "column",
		indexLabel: "{y}",
		indexLabelFontSize: 18,
		dataPoints: [
			{ label: "2017",  y: 64.26},
			{ label: "2018", y: 54.51},
			{ label: "2019", y: 65.58},
			{ label: "2020", y: 80.37}
		]
	}
	]
};
    $("#chartContainer").CanvasJSChart(options);

    var achievement_2 = {
        title: {
            text: "Touch Point Performance"
		},
		axisY:{
			minimum: 0,
			maximum: 100,
		},
		axisX: {
            title: " ",
            labelFontSize: 16,
            labelWrap: true
        },
		colorSet: "flexmonsterStyle",
        exportEnabled: true,
        animationEnabled: true,
        data: [{
            type: "column",
            legendText: "{label}",
            indexLabelFontSize: 18,
            indexLabel: "{y}",
            dataPoints: [
                { y: 87.81, label: "Satpam" },
                { y: 79.97, label: "Penaksir" },
                { y: 78.17, label: "kasir" },
				{ y: 53.58, label: "Telepon" },
				{ y: 80.83, label: "Pengelola Agunan" },
				{ y: 94.24, label: "Kebersihan" },
				{ y: 71.4, label: "New Normal" },
				{ y: 80.48, label: "RO" },
            ]
        }]
    };
    $("#achievement_2").CanvasJSChart(achievement_2);

    
    function toogleDataSeries(e){
        if (typeof(e.dataSeries.visible) === "undefined" || e.dataSeries.visible) {
            e.dataSeries.visible = false;
        } else{
            e.dataSeries.visible = true;
        }
        e.chart.render();
    }
    
    }
    </script>
<%- include ('./partials/footer') %>
<script src="https://canvasjs.com/assets/script/jquery.canvasjs.min.js"></script>
<!-- <script src="<% baseurl() %>js/chart.js" type="text/javascript"></script> -->
</html>