<script src="<%= baseurl() %>plugin/flexmonster/flexmonster.js"></script>
<script type="text/javascript">
			var pivot = new Flexmonster({
				container: "pivotContainer",
				toolbar: true,
				height: "100%",
				report: {
					dataSource: {
						filename: "<%= baseurl() %>rawdatacsv/gadai/detail_performance4.csv"
					},
					"slice": {
						"reportFilters": [
							{
								"uniqueName": "AREA"
							},
							{
								"uniqueName": "CABANG"
							},
							{
								"uniqueName": "REGION"
							}
						],
						"columns": [
							{
								"uniqueName": "[Measures]"
							},
							{
								"uniqueName": "Aspek"
							},
							{
								"uniqueName": "Detail Aspek",
								"filter": {
									"exclude": [
										"detail aspek.[total satpam (kondisi 1)]",
										"detail aspek.[total penaksir (kondisi 1)]",
										"detail aspek.[total kasir (kondisi 1)]",
										"detail aspek.[total kebersihan (kondisi 1)]",
										"detail aspek.[total frontliner (kondisi 1) negatif jadi dapat skor]",
										"detail aspek.[total ro (kondisi 1)]",
										"detail aspek.[total kegiatan ro (kondisi 1)]"
									]
								}
							}
						],
						"measures": [
							{
								"uniqueName": "Total",
								"aggregation": "average"
							}
						]
					},
					"options": {
						"viewType": "charts",
						"grid": {
							"showGrandTotals": "off"
						},
						"chart": {
							"showDataLabels": true
						}
