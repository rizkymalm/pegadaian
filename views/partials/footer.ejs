<script type="text/javascript" src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
<script src="<%= baseurl() %>js/angular/angular.js" type="text/javascript"></script>

<!-- <script src="<%= baseurl() %>plugin/flexmonster/lib/jquery.min.js"></script> -->
<script>
$(document).ready(function(){
    $('.rSide .head').css({
        "height" : $('.lSide .head').height() +"px"
    })
    $('.wrapper').css({
        "height" : $(window).height() +"px"
    })
    $('.value-control li').click(function() {
        var ctrltext = $(this).data("select")
        $('.text-control').text(ctrltext)
    })
    $('.square').css({
        "height" : $('.square').width() +"px"
    })
})
function getAreaByKanwil(){
    var kanwil = $('#kanwil').val()
    $.ajax({
        cache: true,
        url: "<%= baseurl() %>acv/areabykanwil/"+kanwil,
        success: function(select){
            $('#area').html(select);
        }
    })
}

function getBranchByArea(){
    var wave = $('#wave').val()
    var area = $('#area').val()
    $.ajax({
        cache: true,
        url: "<%= baseurl() %>acv/branchbyarea/"+area+"/"+wave,
        success: function(select){
            $('#cabang').html(select);
        }
    })
}

function getElementByAspek(){
    var aspek = $('#aspek').val()
    $.ajax({
        cache: true,
        url: "<%= baseurl() %>report/elementbyaspek/"+aspek,
        success: function(select){
            $('#element').html(select);
        }
    })
}
</script>
<script type="text/javascript" src="<%= baseurl() %>js/resize.window.js"></script>
<script type="text/javascript" src="<%= baseurl() %>js/animate.js"></script>